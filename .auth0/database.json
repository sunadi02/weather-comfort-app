{
  "name": "Username-Password-Authentication",
  "enabled_clients": [
    "Weather Comfort Dashboard"
  ],
  "options": {
    "passwordPolicy": "good",
    "password_history": {
      "enable": false,
      "size": 5
    },
    "password_no_personal_info": {
      "enable": true
    },
    "password_dictionary": {
      "enable": true
    },
    "password_complexity_options": {
      "min_length": 8
    },
    "enabledDatabaseCustomization": false,
    "brute_force_protection": true,
    "import_mode": false,
    "disable_signup": true,
    "requires_username": false,
    "validation": {
      "username": {}
    }
  }
}
